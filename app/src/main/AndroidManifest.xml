<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.six.carrental">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".View.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".View.MainActivity" />
        <activity android:name=".View.MyReservationActivity" />
        <activity android:name=".View.RegisterActivity" />
        <activity android:name=".View.ReservationActivity" />
        <activity android:name=".View.SetPasswordActivity" />
        <activity android:name=".View.PublishActivity" />
        <activity android:name=".View.SetPublishActivity" />
        <activity android:name=".View.SetInfoActivity" />
        <activity android:name=".View.ShowOrderActivity"></activity>
    </application>

</manifest>